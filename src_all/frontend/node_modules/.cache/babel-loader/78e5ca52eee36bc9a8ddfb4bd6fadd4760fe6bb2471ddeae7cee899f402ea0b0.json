or) {\r\n                // 网络或其他错误处理\r\n                registerError.value = '注册过程中发生错误。' + error.message;\r\n            }\r\n        };\r\n\r\n        if (output.value === '注册成功') {\r\n            router.push('/login'); // 注册成功后跳转到登录页面\r\n        };\r\n\r\n        // 返回响应式引用供模板使用\r\n        return {\r\n            username,\r\n            password,\r\n            confirmPassword,\r\n            registerError,\r\n            output,\r\n            handleSubmit\r\n        };\r\n\r\n\r\n    }\r\n};\r\n</script>"],"mappings":";AAwBA,OAAOA,KAAI,MAAO,OAAO;AACzB,SAASC,GAAE,QAAS,KAAK;AACzB,SAASC,SAAQ,QAAS,YAAY;AAEtC,eAAe;EACXC,KAAKA,CAAA,EAAG;IACJ,MAAMC,QAAO,GAAIH,GAAG,CAAC,EAAE,CAAC;IACxB,MAAMI,QAAO,GAAIJ,GAAG,CAAC,EAAE,CAAC;IACxB,MAAMK,eAAc,GAAIL,GAAG,CAAC,EAAE,CAAC;IAC/B,MAAMM,aAAY,GAAIN,GAAG,CAAC,EAAE,CAAC,EAAE;IAC/B,MAAMO,MAAK,GAAIP,GAAG,CAAC,EAAE,CAAC;IACtB,MAAMQ,MAAK,GAAIP,SAAS,CAAC,CAAC;IAE1B,MAAMQ,YAAW,GAAI,MAAAA,CAAA,KAAY;MAC7B,IAAIL,QAAQ,CAACM,KAAI,KAAML,eAAe,CAACK,KAAK,EAAE;QAC1CJ,aAAa,CAACI,KAAI,GAAI,aAAa;QACnC;MACJ;MAEA,IAAI;QACA,MAAMC,QAAO,GAAI,MAAMZ,KAAK,CAACa,IAAI,CAAC,oCAAoC,EAAE;UACpET,QAAQ,EAAEA,QAAQ,CAACO,KAAK;UACxBN,QAAQ,EAAEA,QAAQ,CAACM;QACvB,CAAC,CAAC;QACF,IAAIC,QAAQ,CAACE,MAAK,KAAM,GAAG,EAAE;UACzBN,MAAM,CAACG,KAAI,GAAIC,QAAQ,CAACG,IAAI,EAAE;UAC9B;UACA,IAAIH,QAAQ,CAACG,IAAG,KAAM,MAAM,EAAE;YAC1BC,KAAK,CAAC,MAAM,CAAC;UACjB;UACA;QACJ;MACJ,EAAE,OAAOC,KAAK,EAAE;QACZ;QACAV,aAAa,CAACI,KAAI,GAAI,YAAW,GAAIM,KAAK,CAACC,OAAO;MACtD;IACJ,CAAC;IAED,IAAIV,MAAM,CAACG,KAAI,KAAM,MAAM,EAAE;MACzBF,MAAM,CAACU,IAAI,CAAC,QAAQ,CAAC,EAAE;IAC3B;IAAC;;IAED;IACA,OAAO;MACHf,QAAQ;MACRC,QAAQ;MACRC,eAAe;MACfC,aAAa;MACbC,MAAM;MACNE;IACJ,CAAC;EAGL;AACJ,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}